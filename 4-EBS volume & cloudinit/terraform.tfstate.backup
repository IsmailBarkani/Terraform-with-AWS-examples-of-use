            "part": [
              {
                "content": "#cloud configuration https://cloudinit.readthedocs.io/en/latest/topics/examples.html\r\nrepo_update: true\r\nrepo_apgrade: all\r\n\r\npackages:\r\n    -docker.io\r\n    -lvm2\r\n\r\n\r\n",
                "content_type": "text/cloud-config",
                "filename": "init.cfg",
                "merge_type": ""
              },
              {
                "content": "#!/bin/bash\nvgchange -ay\n\nDEVICE_FS=`blkid -o value -s TYPE /dev/xvdh` #these going to retur the FS of DEVICE\nif [\"`echo -n $DEVICE_FS`\"==\"\"];then #if it dosen't contient a file system\n        pvcreate /dev/xvdh\n        vgcreate data /dev/xvdh #create volume group data\n        lvcreate --name volume1 -l -100%FREE data  #create logical volume volume1\n        mkfs.ext4 /dev/data/volume1 #create file system ext4\nfi\nmkdir -p /data #create data directory, -p if exist don't create\necho '/dev/data/volume1 /data ext4 defaults 0 0' \u003e\u003e /etc/fastab\nmount /data",
                "content_type": "text/x-shellscript",
                "filename": "",
                "merge_type": ""
              }
            ],
            "rendered": ""
          }
        }
